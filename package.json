	"browserslist": [
		"last 3 versions",
		"> 1%",
		"edge 16",
		"firefox 52",
		"chrome 57",
		"safari 10.1",
		"opera 44",
		"ios_saf 10.3",
		"samsung 6.2"
	],
	"postcssSortingConfig": {
		"order": [
			"dollar-variables",
			"at-rules",
			"custom-properties",
			"declarations",
			{
				"type": "at-rule",
				"name": "include",
				"parameter": "breakpoint"
			},
			{
				"type": "at-rule",
				"name": "media"
			},
			"rules"
		],
		"properties-order": "alphabetical",
		"unspecified-properties-position": "bottom"
	}
